@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<div class="container-fluid">
    <!-- ============================================================== -->
    <!-- Start Page Content -->
    <!-- ============================================================== -->
    <!-- Row -->
    <div class="row">
        <!-- Column -->
        <div class="col-lg-4 col-xlg-3 col-md-12">
            <div class="white-box">
                <div class="user-bg">
                    <img width="100%" alt="user" src="~/assets/plugins/images/large/img1.jpg">
                    <div class="overlay-box">
                        <div class="user-content">
                            <a href="javascript:void(0)">
                                <img src="~/Images/UsersImages/@Model.OwnerInfo.UserProfileImageName"
                                     class="thumb-lg img-circle" alt="img">
                            </a>
                            <h4 class="text-white mt-2">@Model.OwnerInfo.UserName</h4>
                            <h5 class="text-white mt-2">@Model.OwnerInfo.Email</h5>
                        </div>
                    </div>
                </div>
                <div class="user-btm-box mt-5 d-md-flex">
                    <div class="col-md-4 col-sm-4 text-center">
                        <h1>Apps Count : </h1>
                    </div>
                    <div class="col-md-4 col-sm-4 text-center">
                        <h1>@Model.OwnerInfo.AppCount</h1>
                    </div>
                </div>
            </div>
        </div>
        <!-- Column -->
        <!-- Column -->
